{
  "name": "developers-blog",
  "private": true,
  "description": "Every developer needs a simple blog",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn && gatsby build",
    "develop": "yarn && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "storybook": "yarn && start-storybook -p 6006 --ci",
    "test": "jest",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "dependencies": {
    "@types/jest": "^25.1.0",
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^8.2.0",
    "classnames": "^2.2.6",
    "flexboxgrid-sass": "^8.0.5",
    "front-matter-markdown": "^0.4.4",
    "gatsby": "^2.13.28",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-force-file-loader": "^0.1.0",
    "gatsby-plugin-postcss-sass": "^1.0.22",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-typescript": "^2.1.26",
    "imports-loader": "^0.8.0",
    "markdown-it": "^10.0.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "typescript": "^3.7.5",
    "yaml-front-matter": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-options": "^5.1.9",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/react": "^5.2.5",
    "@types/classnames": "^2.2.9",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qmachard/developers-blog"
  },
  "bugs": {
    "url": "https://github.com/qmachard/developers-blog/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && git add ."
    }
  }
}
