{
  "name": "developers-blog",
  "private": true,
  "description": "Every developer needs a simple blog",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn && gatsby build",
    "develop": "yarn && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "storybook": "yarn && start-storybook -p 6006 --ci",
    "test": "jest",
    "lint": "eslint .",
    "deploy": "yarn && gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "@octokit/graphql": "^4.3.1",
