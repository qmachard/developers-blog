    "jest": "^25.1.0",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "**/**/minimist": "^1.2.3",
    "**/**/acorn": "^6.4.1",
    "**/**/kind-of": "^6.0.3"
